<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="JmixDataStore">
        <createTable tableName="SOURCE_DB">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_SOURCE_DB"/>
            </column>
            <column name="NAME" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="HOST" type="VARCHAR(255)"/>
            <column name="URL" type="VARCHAR(255)"/>
            <column name="PORT" type="INT"/>
            <column name="USERNAME" type="VARCHAR(255)"/>
            <column name="PASSWORD" type="VARCHAR(255)"/>
            <column name="DBNAME" type="VARCHAR(255)"/>
            <column name="DATATYPE" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>